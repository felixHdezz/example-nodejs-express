doctype html
html(lang ="es")
	head
		title=titulo
		link(href="css/style.css",rel='stylesheet')
	body
		h1 
			a(href="http://tiendafrutas.com")=titulo
		div.contenedor
			- for(var i = 0; i < camisetas.length; ++i){
				div.item  
					h2="camisetas "+ camisetas[i].Titulo
					img(src=camisetas[i].imagen)
					a(href="/tienda/comprar/"+camisetas[i].Titulo).comprar Comprar
			- }
